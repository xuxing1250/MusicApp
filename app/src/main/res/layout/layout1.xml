<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="horizontal">
    <RelativeLayout
        android:id="@+id/titil"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:background="@drawable/skin_kg_navigation_local_entry_bg"
        >
        <RelativeLayout
            android:id="@+id/imageview_group"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginStart="26dp"
            android:layout_marginTop="26dp"
            android:layout_alignEnd="@+id/line_view">
            <LinearLayout
                android:id="@+id/lay_group"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:orientation="horizontal"
                >
                <ImageView
                    android:id="@+id/image_like"
                    android:layout_width="30dp"
                    android:layout_height="30dp"
                    android:src="@drawable/b3l"
                    android:layout_weight="1"
                    />
               <Button
                   android:layout_width="30dp"
                   android:layout_height="30dp"
                   android:visibility="invisible"
                   android:layout_weight="1"

                   />

                <ImageView
                    android:id="@+id/image_list"
                    android:layout_width="30dp"
                    android:layout_height="30dp"
                    android:src="@drawable/b4l"
                    android:layout_weight="1"

                    />
                <Button
                    android:layout_width="30dp"
                    android:layout_height="30dp"
                    android:visibility="invisible"
                    android:layout_weight="1"
                    />


                <ImageView
                    android:id="@+id/image_download"
                    android:layout_width="30dp"
                    android:layout_height="30dp"
                    android:src="@drawable/b3k"
                    android:layout_weight="1"

                    />
                <Button
                    android:layout_width="30dp"
                    android:layout_height="30dp"
                    android:visibility="invisible"
                    android:layout_weight="1"

                    />
                <ImageView
                    android:id="@+id/image_recent"
                    android:layout_width="30dp"
                    android:layout_height="30dp"
                    android:src="@drawable/efl"
                    android:layout_weight="1"
                    />
            </LinearLayout>
            <LinearLayout
                android:id="@+id/lay_group2"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginTop="6dp"
                android:orientation="horizontal"
                android:layout_below="@+id/lay_group"
                >
                <TextView
                    android:id="@+id/text_like"
                    android:layout_width="40dp"
                    android:layout_height="wrap_content"
                    android:text="收藏"
                    android:textSize="8pt"
                    android:textColor="#ffffff"
                    android:layout_weight="1"
                    android:gravity="center"
                    />
                <Button
                    android:layout_width="40dp"
                    android:layout_height="40dp"
                    android:visibility="invisible"
                    android:layout_weight="1"
                    />

                <TextView
                    android:id="@+id/text_list"
                    android:layout_width="40dp"
                    android:layout_height="wrap_content"
                    android:text="歌单"
                    android:textSize="8pt"
                    android:textColor="#ffffff"
                    android:layout_weight="1"
                    android:gravity="center"

                    />
                <Button
                    android:layout_width="40dp"
                    android:layout_height="40dp"
                    android:visibility="invisible"
                    android:layout_weight="1"
                    />
                <TextView
                    android:id="@+id/text_download"
                    android:layout_width="40dp"
                    android:layout_height="wrap_content"
                    android:text="下载"
                    android:textSize="8pt"
                    android:textColor="#ffffff"
                    android:layout_weight="1"
                    android:gravity="center"

                    />
                <Button
                    android:layout_width="40dp"
                    android:layout_height="40dp"
                    android:visibility="invisible"
                    android:layout_weight="1"
                    />
                <TextView
                    android:id="@+id/text_recent"
                    android:layout_width="40dp"
                    android:layout_height="wrap_content"
                    android:text="最近"
                    android:textSize="8pt"
                    android:textColor="#ffffff"
                    android:layout_weight="1"
                    android:gravity="center"

                    />


            </LinearLayout>

        </RelativeLayout>
        <View
            android:id="@+id/line_view"
            android:layout_width="match_parent"
            android:layout_height="0.5dp"
            android:background="#cccccc"
            android:layout_marginStart="24dp"
            android:layout_marginEnd="24dp"
            android:layout_below="@+id/imageview_group" />
        <RelativeLayout
            android:id="@+id/lay_sdcadr"
            android:layout_width="fill_parent"
            android:layout_height="wrap_content"
            android:layout_below="@id/line_view"
            android:layout_marginTop="10dp"
            android:layout_marginBottom="10dp"
            >
            <TextView
                android:id="@+id/text_sdCard"
                android:layout_width="fill_parent"
                android:layout_height="wrap_content"
                android:text="本地音乐"
                android:textSize="8pt"
                android:layout_marginStart="26dp"
                android:textColor="#ffffff"
                />
        </RelativeLayout>
        <RelativeLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:layout_below="@+id/lay_sdcadr"
            android:background="#ffffff"
            android:id="@+id/relativeLayout2">
            <ImageView
                android:id="@+id/image_songData"
                android:layout_width="56dp"
                android:layout_height="56dp"
                android:src="@drawable/skin_kg_navigation_yueku_normal"
                android:layout_alignParentTop="true"
                android:layout_alignParentStart="true"
                android:layout_marginStart="40dp"
                android:layout_marginTop="16dp"
                />
            <ImageView
                android:id="@+id/image_channel"
                android:layout_width="56dp"
                android:layout_height="56dp"
                android:src="@drawable/skin_kg_navigation_channel_normal"
                android:layout_alignTop="@+id/image_songData"
                android:layout_centerHorizontal="true" />


            <ImageView
                android:id="@+id/image_groups"
                android:layout_width="56dp"
                android:layout_height="56dp"
                android:src="@drawable/skin_kg_navigation_kuchat_normal"
                android:layout_marginEnd="40dp"
                android:layout_alignTop="@+id/image_songData"
                android:layout_alignParentEnd="true" />
            <TextView
                android:id="@+id/text_songData"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="乐库"
                android:textSize="10pt"
                android:textColor="#aaaaaa"
                android:layout_below="@+id/image_songData"
                android:layout_alignStart="@+id/image_songData"
                android:layout_marginStart="6dp"
                android:layout_marginTop="10dp"
                />
            <TextView
                android:id="@+id/text_channel"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="电台"
                android:textSize="10pt"
                android:textColor="#aaaaaa"
                android:layout_alignTop="@+id/text_songData"
                android:layout_centerHorizontal="true" />
            <TextView
                android:id="@+id/text_groups"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="群组"
                android:textSize="10pt"
                android:textColor="#aaaaaa"
                android:layout_alignTop="@+id/text_channel"
                android:layout_alignEnd="@+id/image_groups"
                android:layout_marginEnd="6dp"
                />
            <View
                android:id="@+id/line_view2"
                android:layout_width="match_parent"
                android:layout_height="0.5dp"
                android:background="#eeeeee"
                android:layout_marginTop="20dp"
                android:layout_marginStart="24dp"
                android:layout_marginEnd="24dp"
                android:layout_below="@+id/text_groups" />


        </RelativeLayout>
    </RelativeLayout>
</RelativeLayout>


